        "12": "3rem",
        "16": "4rem",
        "20": "5rem",
        "24": "6rem",
        "32": "8rem"
      }
    },
    
    "borderRadius": {
      "system": "sharp",
      "none": "0",
      "sm": "0",
      "base": "0.125rem",
      "md": "0.25rem",
      "lg": "0.5rem",
      "xl": "1rem",
      "full": "9999px"
    },
    
    "shadows": {
      "system": "bold",
      "none": "none",
      "sm": "0 2px 4px 0 rgba(0, 0, 0, 0.1)",
      "base": "0 4px 6px -1px rgba(0, 0, 0, 0.1), 0 2px 4px -1px rgba(0, 0, 0, 0.06)",
