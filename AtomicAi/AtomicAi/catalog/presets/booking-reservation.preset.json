{
  "presetId": "booking-reservation",
  "label": "Booking/Reservation with Calendar + Resources + Payments optional",
  "productType": "BOOKING",
  "delivery": {
    "mode": "multiTenantSaaS",
    "deployment": "cloudNative",
    "architecture": "modularMonolith",
    "pwa": true,
    "desktop": "none"
  },
  "businessModel": {
    "type": "freemium",
    "plans": ["Free", "Pro"]
  },
  "themePack": "minimal-saas",
  "density": "comfortable",
  "pages": {
    "marketing": ["page.landing.saas", "page.pricing.full"],
    "app": ["page.app.auth", "page.app.dashboard", "page.app.entityCrud", "page.app.workflow"]
  },
  "patterns": ["pattern.calendarScheduler", "pattern.dashboardReports"],
  "modules": [
    "core.auth",
    "core.rbac",
    "core.tenants",
    "core.auditLogs",
    "core.settings",
    "booking.resources",
    "booking.slots",
    "booking.reservations",
    "booking.notifications"
  ],
  "entities": ["Resource", "TimeSlot", "Reservation", "Customer", "User", "Role", "Tenant"],
  "assumptions": [
    "Calendar views enabled",
    "Payment integration optional"
  ],
  "version": "1.0.0"
}

