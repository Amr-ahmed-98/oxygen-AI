{
  "id": "theme-ecommerce-premium",
  "name": "E-commerce Premium",
  "description": "Premium e-commerce theme with conversion-optimized design",
  "category": ["ecommerce", "premium", "retail"],
  "tags": ["ecommerce", "shop", "retail", "product"],
  
  "foundations": {
    "colors": {
      "primary": {
        "50": "#f0f9ff",
        "100": "#e0f2fe",
        "200": "#bae6fd",
        "300": "#7dd3fc",
        "400": "#38bdf8",
        "500": "#0ea5e9",
        "600": "#0284c7",
        "700": "#0369a1",
        "800": "#075985",
        "900": "#0c4a6e"
      },
      "accent": {
        "50": "#fef3c7",
        "100": "#fde68a",
        "500": "#f59e0b",
        "600": "#d97706",
        "700": "#b45309"
      },
      "neutral": {
        "50": "#fafafa",
        "100": "#f5f5f5",
        "200": "#e5e5e5",
        "300": "#d4d4d4",
        "400": "#a3a3a3",
        "500": "#737373",
        "600": "#525252",
        "700": "#404040",
        "800": "#262626",
        "900": "#171717"
      },
      "semantic": {
        "success": {
          "50": "#f0fdf4",
          "500": "#22c55e",
          "600": "#16a34a"
        },
        "warning": {
          "50": "#fffbeb",
          "500": "#f59e0b",
          "600": "#d97706"
        },
        "error": {
          "50": "#fef2f2",
          "500": "#ef4444",
          "600": "#dc2626"
        },
        "info": {
          "50": "#eff6ff",
          "500": "#3b82f6",
          "600": "#2563eb"
        }
      },
      "background": {
        "default": "#ffffff",
        "secondary": "#fafafa",
        "tertiary": "#f5f5f5"
      },
      "text": {
        "primary": "#171717",
        "secondary": "#525252",
        "tertiary": "#737373",
        "inverse": "#ffffff"
      }
    },
    
    "typography": {
      "fontFamily": {
        "sans": ["Poppins", "Inter", "system-ui", "sans-serif"],
        "serif": ["Playfair Display", "Georgia", "serif"],
        "mono": ["JetBrains Mono", "monospace"]
      },
      "fontSize": {
        "xs": "0.75rem",
        "sm": "0.875rem",
        "base": "1rem",
        "lg": "1.125rem",
        "xl": "1.25rem",
        "2xl": "1.5rem",
        "3xl": "1.875rem",
        "4xl": "2.25rem",
        "5xl": "3rem",
        "6xl": "3.75rem"
      },
      "fontWeight": {
        "light": "300",
        "normal": "400",
        "medium": "500",
        "semibold": "600",
        "bold": "700",
        "extrabold": "800"
      },
      "lineHeight": {
        "tight": "1.2",
        "normal": "1.5",
        "relaxed": "1.75"
      }
    },
    
    "spacing": {
      "scale": "1.25",
      "base": "1rem",
      "values": {
        "0": "0",
        "1": "0.25rem",
        "2": "0.5rem",
        "3": "0.75rem",
        "4": "1rem",
        "5": "1.25rem",
        "6": "1.5rem",
        "8": "2rem",
        "10": "2.5rem",
        "12": "3rem",
        "16": "4rem",
        "20": "5rem",
        "24": "6rem",
        "32": "8rem"
      }
    },
    
    "borderRadius": {
      "system": "rounded",
      "none": "0",
      "sm": "0.25rem",
      "base": "0.375rem",
      "md": "0.5rem",
      "lg": "0.75rem",
      "xl": "1rem",
      "2xl": "1.5rem",
      "full": "9999px"
    },
    
    "shadows": {
      "system": "elevated",
      "none": "none",
      "sm": "0 1px 2px 0 rgba(0, 0, 0, 0.05)",
      "base": "0 1px 3px 0 rgba(0, 0, 0, 0.1), 0 1px 2px 0 rgba(0, 0, 0, 0.06)",
      "md": "0 4px 6px -1px rgba(0, 0, 0, 0.1), 0 2px 4px -1px rgba(0, 0, 0, 0.06)",
      "lg": "0 10px 15px -3px rgba(0, 0, 0, 0.1), 0 4px 6px -2px rgba(0, 0, 0, 0.05)",
      "xl": "0 20px 25px -5px rgba(0, 0, 0, 0.1), 0 10px 10px -5px rgba(0, 0, 0, 0.04)",
      "2xl": "0 25px 50px -12px rgba(0, 0, 0, 0.25)"
    },
    
    "motion": {
      "duration": {
        "fast": "150ms",
        "base": "200ms",
        "slow": "300ms",
        "slower": "500ms"
      },
      "easing": {
        "easeIn": "cubic-bezier(0.4, 0, 1, 1)",
        "easeOut": "cubic-bezier(0, 0, 0.2, 1)",
        "easeInOut": "cubic-bezier(0.4, 0, 0.2, 1)"
      }
    }
  },
  
  "density": {
    "mode": "comfortable",
    "spacingMultiplier": 1.25,
    "componentHeight": "standard"
  },
  
  "darkMode": {
    "enabled": true,
    "strategy": "class",
    "colors": {
      "background": {
        "default": "#0f172a",
        "secondary": "#1e293b",
        "tertiary": "#334155"
      },
      "text": {
        "primary": "#f1f5f9",
        "secondary": "#cbd5e1",
        "tertiary": "#94a3b8"
      }
    }
  },
  
  "rtl": {
    "enabled": true,
    "supported": true
  },
  
  "ecommerce": {
    "productGrid": {
      "columns": {
        "mobile": 2,
        "tablet": 3,
        "desktop": 4
      }
    },
    "cart": {
      "sticky": true,
      "position": "right"
    }
  }
}

